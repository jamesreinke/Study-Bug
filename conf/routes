# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

POST	/auth/loginpost				controllers.Authentication.loginPost

POST 	/auth/register				controllers.Authentication.registerPost

POST 	/auth/forget 				controllers.Authentication.forgetPost

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file: String)
